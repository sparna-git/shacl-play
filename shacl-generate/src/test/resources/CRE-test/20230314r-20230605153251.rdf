      <skos:notation rdf:datatype="https://data.europarl.europa.eu/def/epvoc#speechId">2-385-0000</skos:notation>
      <epvoc:originalLanguage rdf:resource="http://publications.europa.eu/resource/authority/language/DE"/>
      <eli:is_realized_by>
         <eli:Expression rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-SPEECH-2-385-0000/de">
            <eli:language rdf:resource="http://publications.europa.eu/resource/authority/language/DE"/>
            <epvoc:expressionContent rdf:parseType="Literal">
               <p>Ich muss mich bei dem Kollegen Wallace entschuldigen. Er wollte eigentlich eine Frage stellen, und ich habe gedacht, ich würde von den Diensten darauf hingewiesen, dass Herr Bütikofer schon über der Redezeit war. Deshalb ist der Moment jetzt leider durch mich verpasst worden.</p>
            </epvoc:expressionContent>
         </eli:Expression>
      </eli:is_realized_by>
      <eli:is_part_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-ITM-17"/>
   </eli:WorkSubdivision>
