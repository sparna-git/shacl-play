   <eli:WorkSubdivision rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-SPEECH-2-283-0000">
      <dcterms:identifier>CRE-9-2023-03-14-SPEECH-2-283-0000</dcterms:identifier>
      <epvoc:numbering>2-283-0000</epvoc:numbering>
      <skos:notation rdf:datatype="https://data.europarl.europa.eu/def/epvoc#speechId">2-283-0000</skos:notation>
      <epvoc:originalLanguage rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
      <eli:is_realized_by>
         <eli:Expression rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-SPEECH-2-283-0000/en">
            <eli:language rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
            <epvoc:expressionContent rdf:parseType="Literal">
               <p> – I’ll take the chance of a follow-up question specifically on the JCPOA and the fact that we see nuclear enrichment now in Iran. Are there any consultations between the EU and the US on how either the JCPOA or an alternative plan to deal with this is being pursued?</p>
            </epvoc:expressionContent>
         </eli:Expression>
      </eli:is_realized_by>
      <eli:is_part_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-ITM-14"/>
