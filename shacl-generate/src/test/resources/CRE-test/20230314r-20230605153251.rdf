      <eli-dl:had_activity_type rdf:resource="http://publications.europa.eu/resource/authority/event/EP_PENARY_DEBATE_SPEECH"/>
      <eli-dl:activity_start_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-03-14T18:56:59.991</eli-dl:activity_start_date>
