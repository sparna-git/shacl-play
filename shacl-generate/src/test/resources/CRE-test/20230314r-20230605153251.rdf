      <eli-dl:activity_date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-03-14</eli-dl:activity_date>
      <eli-dl:recorded_in_a_realization_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-14-SPEECH-2-459-0000"/>
