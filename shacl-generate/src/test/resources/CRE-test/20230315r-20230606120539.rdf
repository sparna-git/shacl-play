      <epvoc:numbering>3-402-0000</epvoc:numbering>
      <skos:notation rdf:datatype="https://data.europarl.europa.eu/def/epvoc#speechId">3-402-0000</skos:notation>
      <epvoc:originalLanguage rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
      <eli:is_realized_by>
         <eli:Expression rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-15-SPEECH-3-402-0000/en">
            <eli:language rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
            <epvoc:expressionContent rdf:parseType="Literal">
               <p>
                  <em>Member of the Commission</em>
                  <strong>.</strong>  – Madam President, I thank the honourable Members for this debate.</p>
               <p>Despite our actions, too many human rights defenders remain at risk. We all need to work together – the EEAS, the Commission, the Parliament and the EU Member States – with a sense of urgency to adapt our actions to today’s challenges. We will continue our unwavering efforts to promote a safe and enabling environment for civil society and HRDs, including those active in exile, and continue to condemn any threats or attacks against them.</p>
            </epvoc:expressionContent>
         </eli:Expression>
      </eli:is_realized_by>
      <eli:is_part_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-03-15-ITM-18"/>
   </eli:WorkSubdivision>
