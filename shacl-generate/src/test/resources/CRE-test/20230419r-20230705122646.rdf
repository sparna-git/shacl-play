      <eli-dl:recorded_in_a_realization_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-04-19-SPEECH-3-049-0000"/>
      <eli-dl:had_activity_type rdf:resource="http://publications.europa.eu/resource/authority/event/EP_PENARY_DEBATE_SPEECH"/>
      <eli-dl:activity_start_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-04-19T10:08:20.003</eli-dl:activity_start_date>
      <eli-dl:activity_end_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-04-19T10:09:29.000</eli-dl:activity_end_date>
