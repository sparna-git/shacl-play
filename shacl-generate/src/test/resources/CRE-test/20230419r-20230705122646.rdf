      <eli-dl:had_activity_type rdf:resource="http://publications.europa.eu/resource/authority/event/EP_PENARY_DEBATE_SPEECH"/>
      <eli-dl:activity_start_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-04-19T10:23:35.467</eli-dl:activity_start_date>
