      <eli-dl:had_activity_type rdf:resource="http://publications.europa.eu/resource/authority/event/EP_PENARY_DEBATE_SPEECH"/>
      <eli-dl:activity_start_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2023-06-13T15:22:27.501</eli-dl:activity_start_date>
