{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@context": {
      "$ref": "#/$defs/@context"
    },
    "data": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/$defs/MainNodeShape"
      }
    }
  },
  "additionalProperties": false,
  "required": ["@context", "data"],
  "$defs": {
    "@context": {
      "type": "string",
      "format": "iri-reference"
    },
    "MainNodeShape": {
        "type": "object",
        "properties": {
            "id": {
                "type": "string",
                "format": "iri-reference"
            },
            "customName": {
                "type" : "string"
            }
        },
        "required": ["id"]
    }
  }
}
