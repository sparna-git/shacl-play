{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@context": {
      "$ref": "#/$defs/@context"
    },
    "data": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/$defs/MainNodeShape"
      }
    }
  },
  "additionalProperties": false,
  "required": ["@context", "data"],
  "$defs": {
    "@context": {},
    "MainNodeShape": {
        "type": "object",
        "properties": {
            "id": {
              "type": "string",
              "format": "iri-reference",
              "pattern": "^eli/doc/[0-9]*$"
            },
            "property": {
              "type": "string",
              "pattern": "^org/ep-[0-9]{1,2}$"
            },
            "http://example.org/otherProperty": {
              "type": "string",
              "pattern": "^https://foo.org/org/ep-[0-9]{1,2}$"
            }
        },
        "required": ["id"]
    }
  }
}