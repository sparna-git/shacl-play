@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix play: <https://shacl-play.sparna.fr/ontology#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ex:MainNodeShape
    a sh:NodeShape ;
    rdfs:label "Main Node Shape"@en ;
    sh:property [
        sh:path [ sh:inversePath ex:property ] ;
        sh:nodeKind sh:IRI ;
        sh:node ex:ValueA ;
        play:shortname "inverse_property" ;
    ] ;
.

ex:ValueA
    a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "^https://data.europarl.europa.eu/eli/dl/doc/SP-(?<date>(19[5-9][0-9]|20[0-9][0-9])-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01]))-TA-(?<parliamentaryTerm>[0-9]{1,2})-(?<year>[0-9]{4})-(?<numberProc>[0-9]{4})$" ;
    skos:example "https://data.europarl.europa.eu/eli/dl/doc/SP-2025-03-06-TA-10-2024-0061" ;
.