@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .
@prefix ex-external: <http://example.org/external/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:MainNodeShape
    a sh:NodeShape ;
    sh:targetClass ex:SimpleClass ;
    sh:property [
        sh:path ex:p ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
        sh:or ( [sh:node ex-external:Unknown1] [sh:node ex-external:Unknown2] ) ;
    ] ;
.
