{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@context": {
      "$ref": "#/$defs/@context"
    },
    "data": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/$defs/MainNodeShape"
      }
    }
  },
  "additionalProperties": false,
  "required": ["@context", "data"],
  "$defs": {
    "@context": {},
    "OtherShape": {
      "examples": ["https://data.europarl.europa.eu/eli/dl/doc/A-10-2025-0134"],
      "pattern": "^https://data.europarl.europa.eu/eli/dl/doc/A-(?<parliamentaryTerm>[0-9]{1,2})-(?<year>[0-9]{4})-(?<number>[0-9]{4})$",
      "format": "iri-reference",
      "type": "string"
    },
    "MainNodeShape": {
      "type": "object",
      "required": ["id"],
      "properties": {
        "p": {"$ref": "#/$defs/OtherShape"},
        "id": {
          "format": "iri-reference",
          "type": "string"
        }
      }
    }
  }
}