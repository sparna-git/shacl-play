@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .

ex:MainNodeShape
    a sh:NodeShape ;
    sh:property [
        sh:path ex:integerProperty ;
        sh:datatype <http://www.w3.org/2001/XMLSchema#integer> ;
        sh:maxCount 1 ;
    ] .
